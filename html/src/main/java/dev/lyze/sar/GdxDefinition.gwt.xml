<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
    <source path="" />
	<inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
	<inherits name="com.crashinvaders.vfx.GdxVfxCore" />
	<inherits name="com.crashinvaders.vfx.GdxVfxEffects" />
	<inherits name="com.crashinvaders.vfx.GdxVfxGwt" />
	<inherits name="dev.lyze.sar.Main" />
	<inherits name="libgdx_screenmanager" />
	<inherits name="libgdx_screenmanager_gwt" />
	<inherits name='com.kotcrab.vis.vis-ui' />
	<inherits name='com.artemis.backends.artemis_backends_gwt' />
	<inherits name="jsr305" />
	<inherits name="com.aliasifkhan.hackLights"/>
	<inherits name="space.earlygrey.shapedrawer"/>
	<extend-configuration-property name="artemis.reflect.include" value="dev.lyze.sar.components" />
